<jqassistant-rules
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://schema.jqassistant.org/rule/v2.0"
        xsi:schemaLocation="http://schema.jqassistant.org/rule/v2.0 https://schema.jqassistant.org/rule/jqassistant-rule-v2.0.xsd">

    <group id="AcousticRules">
        <includeGroup refId="architecture"/>
        <includeGroup refId="build"/>
        <includeGroup refId="description"/>
        <includeGroup refId="filter"/>
        <includeGroup refId="logging"/>
        <includeGroup refId="metrics"/>
        <includeGroup refId="modifier"/>
        <includeGroup refId="packages"/>
        <includeGroup refId="selector"/>
    </group>
</jqassistant-rules>