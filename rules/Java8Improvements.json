{
  "name": "Java 8 Improvements",
  "selectors": [
    {
      "@processor": "SelectWithTag",
      "tags": [
        "java8"
      ]
    }
  ],
  "filters": [
    {
      "@processor": "DropWithType",
      "types": [
        "BUG"
      ]
    },
    {
      "@processor": "DropWithTag",
      "tags": [
        "cert",
        "cwe"
      ]
    },
    {
      "@processor": "RemoveDeprecated"
    }
  ],
  "modifier": [
    {
      "@processor": "ChangeSeverity",
      "from": "MINOR",
      "to": "MAJOR"
    }
  ]
}
