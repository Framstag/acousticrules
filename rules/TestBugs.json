{
  "name": "Test Bugs",
  "selectors": [
    {
      "@processor": "SelectWithTag",
      "reason": "Rule regarding testing use of test frameworks",
      "tags": [
        "tests",
        "junit",
        "assertj"
      ]
    }
  ],
  "filters": [
    {
      "@processor": "DropNotWithType",
      "reason": "We are only interested in bugs",
      "types": [
        "BUG"
      ]
    },
    {
      "@processor": "RemoveDeprecated"
    }
  ]
}
